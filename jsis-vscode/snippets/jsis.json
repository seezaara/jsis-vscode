{
    "JSIS If Conditional": {
        "prefix": ":if",
        "body": [
            "<div :if=\"$1\"></div>"
        ],
        "description": "If conditional example"
    },
    "JSIS ElseIf Conditional": {
        "prefix": ":elseif",
        "body": [
            "<div :elseif=\"$1\"></div>"
        ],
        "description": "ElseIf conditional example"
    },
    "JSIS Else Conditional": {
        "prefix": ":else",
        "body": [
            "<div :else></div>"
        ],
        "description": "Else conditional example"
    },
    "JSIS Wait Directive": {
        "prefix": ":wait",
        "body": [
            "<div :wait=\"$1\"></div>\n<div :then=\"data\"></div>\n<div :catch=\"err\">{console.log(err) , \"error\"}</div>"
        ],
        "description": "Add a JSIS wait directive."
    },
    "JSIS For Loop": {
        "prefix": ":for",
        "body": [
            "<div :for=\"var i = 0; i < $1; i++\"></div>"
        ],
        "description": "For loop example"
    },
    "JSIS For Reverse": {
        "prefix": ":forreverse",
        "body": [
            "<div :for=\"var i = $1; i > 0; i--\"></div>"
        ],
        "description": "For loop example"
    },
    "JSIS ForOf Loop": {
        "prefix": "forof",
        "body": [
            "<div :for=\"var item of $1\"></div>"
        ],
        "description": "ForOf loop example"
    },
    "JSIS ForIn Loop": {
        "prefix": ":forin",
        "body": [
            "<div :for=\"var i in $1\"></div>"
        ],
        "description": "ForIn loop example"
    },
    "JSIS Class Directive": {
        "prefix": ":class",
        "body": [
            "<div :class=\"$1\">class</div>"
        ],
        "description": "Class"
    },
    "JSIS Component": {
        "prefix": "comp",
        "body": [
            "<comp src=\"$1\"></comp>"
        ],
        "description": "Component"
    }
}